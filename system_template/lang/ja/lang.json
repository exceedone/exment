            "table_name": "mail_template",
            "table_view_name": "通知テンプレート",
            "description": "通知送信時のメッセージの本文などを管理します。",
            "custom_columns": [
                {
                    "column_name": "mail_key_name",
                    "column_view_name": "通知キー名",
                    "options": {
                        "help": "システム上で、通知テンプレートを一意に判別するためのキー名です。"
                    }
                },
                {
                    "column_name": "mail_view_name",
                    "column_view_name": "通知表示名",
